    "bootstrap": "^4.0.0-beta.2",
    "core-js": "^2.5.1",
    "ngrx-store-localstorage": "^0.2.4",
    "ngx-bootstrap": "^1.9.3",
    "rxjs": "^5.5.2",
    "typescript": "^2.4.2",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.0",
    "@angular/compiler-cli": "^4.2.4",
