    "@angular/platform-browser-dynamic": "10.2.5",
    "@angular/platform-server": "10.2.5",
    "@angular/router": "10.2.5",
    "@ngrx/effects": "^7.3.0",
    "@ngrx/router-store": "^7.3.0",
    "@ngrx/store": "^7.3.0",
    "@ngrx/store-devtools": "^7.3.0",
    "bootstrap": "^4.0.0",
    "core-js": "^2.5.3",
    "ngrx-store-localstorage": "^5.0.0",
    "ngx-bootstrap": "^6.2.0",
    "npm": "^7.19.1",
    "rxjs": "^6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular/cli": "10.2.3",
    "@angular/compiler-cli": "10.2.5",
    "@angular/language-service": "10.2.5",
    "@types/jasmine": "^2.8.6",
    "@types/jasminewd2": "~2.0.3",
