  },
  "author": "Mateusz Psuja",
  "scripts": {
    "precommit": "npm run lint",
    "prepush": "ng test --watch=false",
    "init": "./copyEnvFiles.sh",
    "ng": "ng",
    "start": "ng serve",
    "start:production": "ng serve --env=prod --aot --target=production",
    "build:production": "ng build --env=prod --aot --target=production",
    "build:staging": "ng build --env=staging --aot --target=production",
    "build:workspace": "ng build --env=workspace --aot --target=production",
    "build:dev": "ng build",
    "test": "ng test",
    "test:ci": "ng test --watch=false",
    "lint": "npm run lint:scss && npm run lint:ts && npm run lint:html",
    "lint:ts": "tslint \"src/**/*.ts\"",
    "lint:scss": "sass-lint src/",
    "lint:html": "htmllint -- 'src/**/*.html'",
    "e2e": "ng e2e"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "10.2.5",
    "@angular/common": "10.2.5",
