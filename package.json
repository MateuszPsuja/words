    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "node-sass": "^4.10",
    "protractor": "~7.0.0",
    "rxjs-tslint-rules": "^4.2.0",
    "sass-lint": "^1.12.1",
    "ts-node": "^5.0.0",
    "tslint": "~6.1.0",
    "typescript": "4.0.8"
